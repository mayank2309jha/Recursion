    public static void printFib(int a,int b,int n){
        if(n==0){
            return ;
        }
        int c = a+b;
        System.out.println(c);
        int tempa = b;
        int tempb = c;
        printFib(tempa, tempb, n-1);
    }
    public static void main(String[] args){
        int a = 0;
        int b = 1;
        System.out.println(a);
        System.out.println(b);
        printFib(0,1,5);
    }
